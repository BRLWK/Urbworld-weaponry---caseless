      <damageAmountBase>9</damageAmountBase>
      <stoppingPower>0.5</stoppingPower>
      <speed>40</speed>
    </projectile>
  </ThingDef>

  <ThingDef ParentName="BaseBullet">
    <defName>Bullet_EC_Revolver</defName>
    <label>EC revolver bullet</label>
